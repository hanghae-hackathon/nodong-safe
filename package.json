{
  "name": "nodong-safe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "bun run build && bun --watch src/app.ts",
    "lint": "bunx eslint . --ignore-pattern 'dist/*'",
    "fix": "bunx eslint . --fix --ignore-pattern 'dist/*'",
    "start": "bun run build && NODE_ENV=production bun src/app.ts",
    "prepare": "husky || exit 0"
  },
  "dependencies": {
    "@elysiajs/static": "^1.0.3",
    "dotenv": "^16.4.5",
    "elysia": "^1.0.22",
    "mongoose": "^8.4.1",
    "openai": "^4.47.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.11.0",
    "vite": "^5.2.0"
  }
}
