{
  "name": "bun-playground",
  "module": "src/app.ts",
  "type": "module",
  "scripts": {
    "lint": "bunx eslint .",
    "fix": "bunx eslint . --fix",
    "dev": "bun --watch src/app.ts",
    "build": "NODE_ENV=production bun-vercel ./src/app.ts",
    "start": "NODE_ENV=production bun src/app.ts",
    "prepare": "husky || exit 0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "bun-vercel": "1.0.0-alpha.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.9.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/html": "^1.0.2",
    "elysia": "^1.0.22",
    "mongodb": "^6.7.0",
    "mongoose": "^8.4.0",
    "react": "^18.3.1"
  }
}
